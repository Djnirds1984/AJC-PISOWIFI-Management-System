{
  "name": "ajc-pisowifi",
  "version": "3.3.0",
  "description": "Enterprise-grade PisoWiFi Management System",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "build": "esbuild index.tsx --bundle --outfile=dist/bundle.js --format=esm --minify --sourcemap --loader:.tsx=tsx --loader:.ts=ts --jsx=automatic --define:process.env.NODE_ENV='\"production\"'",
    "postbuild": "node scripts/download_assets.js",
    "dev": "node --watch server.js"
  },
  "dependencies": {
    "esbuild": "^0.20.1",
    "express": "^4.18.2",
    "onoff": "^6.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.12.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.30.5"
  },
  "optionalDependencies": {
    "serialport": "^12.0.0"
  },
  "author": "AJC",
  "license": "ISC"
}
